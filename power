LDA $2A;
STA $40;
DCA;
STA $41;
LDA $2A;
LDX $2B;
STX $43;
DCX;
JZ $18;

ADD $40;
STA $42;
LDA $41; // CARGAS EL ACUMULADOR EN A
DCA;
STA $41; // GUARDAS EL ACUMULADOR EN 41
JZ $1C; // SALTA A LO ULTIMO
LDA  $42;
JNZ $0F; // PRINCIPIO DEL BUCLE

DCX;
LDA $2A;
JZ $28;
DCA;
STA $41;
LDA $42;
STA $40;
JNZ $1B;
HLT;
